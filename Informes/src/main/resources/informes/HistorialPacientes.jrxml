	<query language="sql"><![CDATA[SELECT 
    HistorialMedico.idHistorial, 
    HistorialMedico.idPaciente, 
    Paciente.nombre AS nombrePaciente, 
    Paciente.fechaNacimiento, 
    HistorialMedico.descripcion, 
    HistorialMedico.fecha
